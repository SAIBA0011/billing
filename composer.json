    "keywords": [
        "billing",
        "stripe",
        "braintree",
        "local-billing",
        "subscriptions"        
    ],
    "homepage": "https://github.com/SAIBA0011/billing",
    "license": "MIT",
    "authors": [
        {
            "name": "Gerhard Theunissen",
